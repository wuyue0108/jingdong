em__img\" :src=\"item.imgUrl\">\r\n                <div class=\"product__item__detail\">\r\n                    <h4 class=\"product__item__title\" >{{item.name}}</h4>\r\n                    <p class=\"product__item__price\" >\r\n                        <span class=\"product__item__now\"> &yen;{{item.price}}</span>\r\n                        <span class=\"product__item__origin\">&yen;{{item.oldPrice}}</span>\r\n                    </p>\r\n                </div>\r\n                <div class=\"product__number\">\r\n                    <span \r\n                    @click=\"() => changeCartCountInfo (shopId,item._id,item,-1)\"\r\n                    class=\"product__number__sub\" >-</span>\r\n                    {{item.count || 0}}\r\n                    <span \r\n                    @click=\"() => changeCartCountInfo (shopId,item._id,item,1)\"\r\n                    class=\"product__number__add\" >+</span>\r\n                </div>\r\n            </div>\r\n            </template>\r\n        </div>\r\n        <div class=\"check\">\r\n            <div class=\"check__icon\">\r\n                <img class=\"check__icon__img\" src=\"http://www.dell-lee.com/imgs/vue3/basket.png\" alt=\"\">\r\n                <div class=\"check__icon__tag\">{{countNumber}}</div>\r\n            </div>\r\n            <div class=\"check__info\">\r\n                总计：\r\n                <span class=\"check__info__price\">&yen;{{countPrice}}</span>\r\n            </div>\r\n            <div class=\"check__btn\">\r\n                去结算\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport {computed} from 'vue'\r\nimport {useStore} from 'vuex'\r\nimport {useRoute} from 'vue-router'\r\nimport {useCommonCartEffect} from './commonCartEffect'\r\n\r\nconst useCartEffect = ()=> {\r\n    const { changeCartCountInfo } = useCommonCartEffect();\r\n    const store = useStore();\r\n    const route = useRoute();\r\n    const cartList = store.state.cartList;\r\n    const shopId = route.params.id;\r\n\r\n    const countNumber = computed(() => {\r\n        let totalNumber = 0;\r\n        const productList  = cartList[shopId];\r\n        if(productList){\r\n            for(let i in productList){\r\n                const product = productList[i]\r\n                totalNumber += product.count;\r\n            }\r\n        }\r\n        return totalNumber\r\n    })\r\n    const countPrice = computed(() => {\r\n        let totalPrice = 0;\r\n        const productList  = cartList[shopId];\r\n        if(productList){\r\n            for(let i in productList){\r\n                const product = productList[i]\r\n                totalPrice += product.count * product.price;\r\n            }\r\n        }\r\n        return totalPrice.toFixed(2)\r\n    })\r\n    const productList = computed(() => {\r\n        const productList = cartList[shopId] || []\r\n        return productList\r\n    })\r\n\r\n    const changeCartItemChecked = (shopId,productId) => {\r\n        const changeCartItemChecked = (shopId, productId) => {\r\n        store.commit('changeCartItemChecked', { shopId, productId });\r\n    }\r\n    }\r\n    return {countNumber,countPrice,productList,changeCartCountInfo,changeCartItemChecked}\r\n}\r\nexport default {\r\n    name: 'Cart',\r\n    setup(){\r\n        const route = useRoute();\r\n        const shopId = route.params.id;\r\n        const {countNumber,countPrice,productList,changeCartCountInfo,changeCartItemChecked} = useCartEffect();\r\n    return  {\r\n        countNumber,countPrice,productList,\r\n        changeCartCountInfo,shopId,\r\n        changeCartItemChecked\r\n    }\r\n    }\r\n}\r\n\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.cart{\r\n    position: absolute;\r\n    bottom: 0;\r\n    left: 0;\r\n    right: 0;\r\n    .product{\r\n        background-color: #fff;\r\n    flex: 1;\r\n    overflow-y: scroll;\r\n    &__item{\r\n        display: flex;\r\n        position: relative;\r\n        padding: .09rem 0 .09rem 0;\r\n        margin: 0 .18rem 0 .16rem;\r\n        border-bottom: 1px solid #F1F1F1;\r\n        &__check{\r\n            width: .54rem;\r\n            line-height: .46rem;\r\n            color: #0091FF;\r\n            text-align: center;\r\n            font-size: .2rem;\r\n\r\n        }\r\n        &__img{\r\n            width: .46rem;\r\n            height: .46rem;\r\n            margin-right: .16rem;\r\n        }\r\n        &__title{\r\n            line-height: .2rem;\r\n            margin: 0;\r\n            font-size: .14rem;\r\n            color: #333333;\r\n        }\r\n        &__price{\r\n        margin: .06rem 0;\r\n        line-height: .2rem;\r\n        }\r\n        &__now{\r\n            font-size: .14rem;\r\n            color: #E93B3B;\r\n            margin-right: .06rem;\r\n        }\r\n        &__origin{\r\n            font-size: .12rem;\r\n            color: #999999;\r\n            line-height: .2rem;\r\n            text-decoration: line-through;\r\n        }\r\n    }\r\n    &__number{\r\n        position: absolute;\r\n        bottom: .25rem;\r\n        right: 0;\r\n        font-size: 14px;\r\n        color: #333333;\r\n        line-height: 16px;\r\n        &__add,&__sub{\r\n            display: inline-block;\r\n            width: .2rem;\r\n            height: .2rem;\r\n            border-radius: 50%;\r\n            text-align: center;\r\n            line-height: .18rem;\r\n            font-size: .2rem;\r\n        }\r\n        &__add{\r\n            margin-left: .1rem;\r\n            border: 1px solid #666666 ;\r\n            color: #666666\r\n        }\r\n        &__sub{\r\n            margin-right: .1rem;\r\n            border: 1px solid #0091FF ;\r\n            background: #0091FF;\r\n            color: #FFFFFF;\r\n        }\r\n    }\r\n}\r\n}\r\n.check{\r\n    display: flex;\r\n    &__icon{\r\n        position: relative;\r\n        width: .76rem;\r\n        height: .49rem;\r\n        &__img{\r\n            display: block;\r\n            width: .28rem;