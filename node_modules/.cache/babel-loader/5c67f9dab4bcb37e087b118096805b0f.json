Cb,cAAA,MADa;;AAGnB,kBAAG,MAAM,CAAC,KAAP,KAAiB,CAAjB,IAAsB,MAAM,CAAC,IAAhC,EAAqC;AACrC,gBAAA,WAAW,CAAC,IAAZ,GAAmB,MAAM,CAAC,IAA1B;AACA;;AALmB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KAAJ;;AAAA,oBAAb,cAAa;AAAA;AAAA;AAAA,KAAnB;;AAOA,EAAA,WAAW,CAAC,YAAK;AAAC,IAAA,cAAc;AAAG,GAAxB,CAAX;;AAVgD,gBAWjC,MAAM,CAAC,WAAD,CAX2B;AAAA,MAWzC,IAXyC,WAWzC,IAXyC;;AAahD,SAAO;AAAC,IAAA,IAAI,EAAJ;AAAD,GAAP;AACJ,CAdA;;AAiBA,eAAe;AACX,EAAA,IAAI,EAAE,SADK;AAEX,EAAA,KAFW,mBAEH;AACJ,QAAM,KAAI,GAAI,QAAQ,EAAtB;AACA,QAAM,MAAK,GAAI,KAAK,CAAC,MAAN,CAAa,EAA5B;;AAFI,wBAGiC,YAAY,EAH7C;AAAA,QAGG,UAHH,iBAGG,UAHH;AAAA,QAGe,cAHf,iBAGe,cAHf;;AAAA,gCAIW,oBAAoB,CAAC,UAAD,EAAY,MAAZ,CAJ/B;AAAA,QAIG,IAJH,yBAIG,IAJH;;AAAA,+BAKmC,mBAAmB,EALtD;AAAA,QAKG,QALH,wBAKG,QALH;AAAA,QAKY,mBALZ,wBAKY,mBALZ;;AAMJ,WAAO;AACH,MAAA,UAAU,EAAV,UADG;AACQ,MAAA,UAAU,EAAV,UADR;AACmB,MAAA,cAAc,EAAd,cADnB;AACkC,MAAA,IAAI,EAAJ,IADlC;AAEH,MAAA,QAAQ,EAAR,QAFG;AAEM,MAAA,MAAM,EAAN,MAFN;AAEa,MAAA,mBAAkB,EAAlB;AAFb,KAAP;AAIJ;AAZW,CAAf","sourcesContent":["<template>\r\n    <div class=\"content\">\r\n        <div class=\"category\">\r\n            <div v-for=\"(item,index) in categories\" :key=\"index\"\r\n            @click=\"() => handleTabClick(item.tab)\"\r\n            :class=\"{'category__item':true,'category__item--active':currentTab == item.tab}\"\r\n            >{{item.name}}</div>\r\n        </div>\r\n        <div class=\"product\">\r\n            <div \r\n            class=\"product__item\"\r\n            v-for=\"(item) in List\" :key=\"item._id\"\r\n            >\r\n                <img class=\"product__item__img\" :src=\"item.imgUrl\">\r\n                <div class=\"product__item__detail\">\r\n                    <h4 class=\"product__item__title\" >{{item.name}}</h4>\r\n                    <p class=\"product__item__sales\" >月售 {{item.sales}} 件</p>\r\n                    <p class=\"product__item__price\" >\r\n                        <span class=\"product__item__now\"> &yen;{{item.price}}</span>\r\n                        <span class=\"product__item__origin\">&yen;{{item.oldPrice}}</span>\r\n                    </p>\r\n                </div>\r\n                <div class=\"product__number\">\r\n                    <span \r\n                    @click=\"() => changeCartCountInfo (shopId,item._id,item,-1)\"\r\n                    class=\"product__number__sub\" >-</span>\r\n                    {{cartList?.[shopId]?.[item._id]?.count || 0}}\r\n                    <span \r\n                    @click=\"() => changeCartCountInfo (shopId,item._id,item,1)\"\r\n                    class=\"product__number__add\" >+</span>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport { reactive, ref, toRefs, watchEffect } from 'vue'\r\nimport {useRoute} from 'vue-router'\r\nimport {get} from '../../utils/request'\r\nimport {useCommonCartEffect} from './commonCartEffect'\r\n\r\n\r\n\r\nconst categories = [{\r\n            name:'全部商品',tab:'all'\r\n        },{\r\n            name:'秒杀',tab:'seckill'\r\n        },{\r\n            name:'新鲜水果',tab:'fruit'\r\n        }\r\n]\r\nconst useTabEffect = () => {\r\n    const currentTab  = ref(categories[0].tab)\r\n    const handleTabClick = (tab)=> {\r\n        currentTab.value = tab;\r\n    }\r\n    return {currentTab ,handleTabClick}\r\n}\r\n\r\nconst useCurrentListEffect = (currentTab,shopId) => {\r\n    const contentList = reactive({List:[]})\r\n\r\n    const getContentData = async() => {\r\n        const result = await get(`/api/shop/${shopId}/products`,\r\n        {tab: currentTab.value})\r\n        if(result.errno === 0 && result.data){\r\n        contentList.List = result.data;\r\n        }\r\n    }\r\n    watchEffect(() =>{getContentData()})\r\n    const {List} = toRefs(contentList)\r\n\r\n    return {List}\r\n}\r\n\r\n\r\nexport default {\r\n    name: 'Content',\r\n    setup (){\r\n        const route = useRoute();\r\n        const shopId = route.params.id;\r\n        const {currentTab ,handleTabClick} = useTabEffect();\r\n        const {List} = useCurrentListEffect(currentTab,shopId);\r\n        const {cartList,changeCartCountInfo} = useCommonCartEffect();\r\n        return {\r\n            categories,currentTab,handleTabClick,List,\r\n            cartList,shopId,changeCartCountInfo\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.content{\r\n    display: flex;\r\n    position: absolute;\r\n    left: 0;\r\n    top: 1.5rem;\r\n    bottom: .49rem;\r\n    right: 0;\r\n}\r\n.category{\r\n    overflow-y: scroll;\r\n    width: .76rem;\r\n    background: #F5F5F5;\r\n    // margin-right: .16rem;\r\n    &__item{\r\n        width: 100%;\r\n        line-height: .4rem;\r\n        text-align: center;\r\n        border-radius: 2px;\r\n        font-size: 14px;\r\n        color: #333333;\r\n        &--active{\r\n            background: #FFFFFF;\r\n        }\r\n    }\r\n}\r\n.product{\r\n    flex: 1;\r\n    overflow-y: scroll;\r\n    &__item{\r\n        display: flex;\r\n        position: relative;\r\n        padding: .12rem 0 .12rem 0;\r\n        margin: 0 .18rem 0 .16rem;\r\n        border-bottom: 1px solid #F1F1F1;;\r\n        &__img{\r\n            width: .68rem;\r\n            height: .68rem;\r\n            margin-right: .16rem;\r\n        }\r\n        &__title{\r\n            line-height: .2rem;\r\n            margin: 0;\r\n            font-size: .14rem;\r\n            color: #333333;\r\n        }\r\n        &__sales{\r\n            margin: 0;\r\n            padding: .06rem 0 .06rem 0;\r\n            font-size: .12rem;\r\n            color: #333333;\r\n            line-height: .16rem;\r\n        }\r\n        &__price{\r\n        margin: 0;\r\n        line-height: .2rem;\r\n        }\r\n        &__now{\r\n            font-size: .14rem;\r\n            color: #E93B3B;\r\n            margin-right: .06rem;\r\n        }\r\n        &__origin{\r\n            font-size: .12rem;\r\n            color: #999999;\r\n            line-height: .2rem;\r\n            text-decoration: line-through;\r\n        }\r\n    }\r\n    &__number{\r\n        position: absolute;\r\n        bottom: .12rem;\r\n        right: 0;\r\n        font-size: 14px;\r\n        color: #333333;\r\n        line-height: 16px;\r\n        &__add,&__sub{\r\n            display: inline-block;\r\n            width: .2rem;\r\n            height: .2rem;\r\n            border-radius: 50%;\r\n            text-align: center;\r\n            line-height: .18rem;\r\n            font-size: .2rem;\r\n        }\r\n        &__add{\r\n            margin-left: .1rem;\r\n            border: 1px solid #666666 ;\r\n            color: #666666\r\n        }\r\n        &__sub{\r\n            margin-right: .1rem;\r\n            border: 1px solid #0091FF ;\r\n            background: #0091FF;\r\n            color: #FFFFFF;\r\n        }\r\n    }\r\n}\r\n</style>"],"sourceRoot":""}]}