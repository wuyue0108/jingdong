A,MAAM,WAAU,GAAI,QAAQ,CAAC,YAAM;AAC/B,QAAM,WAAU,GAAI,QAAQ,CAAC,MAAD,CAAR,IAAoB,EAAxC;AACA,WAAO,WAAP;AACH,GAH2B,CAA5B;AAIA,SAAO;AAAC,IAAA,WAAW,EAAX,WAAD;AAAa,IAAA,UAAU,EAAV,UAAb;AAAwB,IAAA,WAAW,EAAX;AAAxB,GAAP;AAEJ,CAlCA;;AAmCA,eAAe;AACX,EAAA,IAAI,EAAE,MADK;AAEX,EAAA,KAFW,mBAEJ;AACH,QAAM,KAAI,GAAI,QAAQ,EAAtB;AACA,QAAM,MAAK,GAAI,KAAK,CAAC,MAAN,CAAa,EAA5B;;AAFG,yBAG0C,aAAa,EAHvD;AAAA,QAGI,WAHJ,kBAGI,WAHJ;AAAA,QAGgB,UAHhB,kBAGgB,UAHhB;AAAA,QAG2B,WAH3B,kBAG2B,WAH3B;;AAAA,+BAI6B,mBAAmB,EAJhD;AAAA,QAIK,mBAJL,wBAIK,mBAJL;;AAKP,WAAQ;AACJ,MAAA,WAAW,EAAX,WADI;AACQ,MAAA,UAAU,EAAV,UADR;AACmB,MAAA,WAAW,EAAX,WADnB;AAEJ,MAAA,mBAAmB,EAAnB,mBAFI;AAEgB,MAAA,MAAK,EAAL;AAFhB,KAAR;AAIA;AAXW,CAAf","sourcesContent":["<template>\r\n    <div class=\"cart\">\r\n        <div class=\"product\">\r\n            <div \r\n            class=\"product__item\"\r\n            v-for=\"(item) in productList\" \r\n            :key=\"item._id\"\r\n            >\r\n                <img class=\"product__item__img\" :src=\"item.imgUrl\">\r\n                <div class=\"product__item__detail\">\r\n                    <h4 class=\"product__item__title\" >{{item.name}}</h4>\r\n                    <p class=\"product__item__price\" >\r\n                        <span class=\"product__item__now\"> &yen;{{item.price}}</span>\r\n                        <span class=\"product__item__origin\">&yen;{{item.oldPrice}}</span>\r\n                    </p>\r\n                </div>\r\n                <div class=\"product__number\">\r\n                    <span \r\n                    @click=\"() => changeCartCountInfo (shopId,item._id,item,-1)\"\r\n                    class=\"product__number__sub\" >-</span>\r\n                    {{item.count || 0}}\r\n                    <span \r\n                    @click=\"() => changeCartCountInfo (shopId,item._id,item,1)\"\r\n                    class=\"product__number__add\" >+</span>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <div class=\"check\">\r\n            <div class=\"check__icon\">\r\n                <img class=\"check__icon__img\" src=\"http://www.dell-lee.com/imgs/vue3/basket.png\" alt=\"\">\r\n                <div class=\"check__icon__tag\">{{countNumber}}</div>\r\n            </div>\r\n            <div class=\"check__info\">\r\n                总计：\r\n                <span class=\"check__info__price\">&yen;{{countPrice}}</span>\r\n            </div>\r\n            <div class=\"check__btn\">\r\n                去结算\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport {computed} from 'vue'\r\nimport {useStore} from 'vuex'\r\nimport {useRoute} from 'vue-router'\r\nimport {useCommonCartEffect} from './commonCartEffect'\r\n\r\nconst useCartEffect = ()=> {\r\n    const store = useStore();\r\n    const route = useRoute();\r\n    const cartList = store.state.cartList;\r\n    const shopId = route.params.id;\r\n\r\n    const countNumber = computed(() => {\r\n        let totalNumber = 0;\r\n        const productList  = cartList[shopId];\r\n        if(productList){\r\n            for(let i in productList){\r\n                const product = productList[i]\r\n                totalNumber += product.count;\r\n            }\r\n        }\r\n        return totalNumber\r\n    })\r\n    const countPrice = computed(() => {\r\n        let totalPrice = 0;\r\n        const productList  = cartList[shopId];\r\n        if(productList){\r\n            for(let i in productList){\r\n                const product = productList[i]\r\n                totalPrice += product.count * product.price;\r\n            }\r\n        }\r\n        return totalPrice.toFixed(2)\r\n    })\r\n    const productList = computed(() => {\r\n        const productList = cartList[shopId] || []\r\n        return productList\r\n    })\r\n    return {countNumber,countPrice,productList}\r\n\r\n}\r\nexport default {\r\n    name: 'Cart',\r\n    setup(){\r\n        const route = useRoute();\r\n        const shopId = route.params.id;\r\n        const {countNumber,countPrice,productList} = useCartEffect();\r\n        const { changeCartCountInfo } = useCommonCartEffect();\r\n    return  {\r\n        countNumber,countPrice,productList,\r\n        changeCartCountInfo,shopId\r\n    }\r\n    }\r\n}\r\n\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.cart{\r\n    position: absolute;\r\n    bottom: 0;\r\n    left: 0;\r\n    right: 0;\r\n    .product{\r\n    flex: 1;\r\n    overflow-y: scroll;\r\n    &__item{\r\n        display: flex;\r\n        position: relative;\r\n        padding: .09rem 0 .09rem 0;\r\n        margin: 0 .18rem 0 .16rem;\r\n        border-bottom: 1px solid #F1F1F1;;\r\n        &__img{\r\n            width: .46rem;\r\n            height: .46rem;\r\n            margin-right: .16rem;\r\n        }\r\n        &__title{\r\n            line-height: .2rem;\r\n            margin: 0;\r\n            font-size: .14rem;\r\n            color: #333333;\r\n        }\r\n        &__price{\r\n        margin: .06rem 0;\r\n        line-height: .2rem;\r\n        }\r\n        &__now{\r\n            font-size: .14rem;\r\n            color: #E93B3B;\r\n            margin-right: .06rem;\r\n        }\r\n        &__origin{\r\n            font-size: .12rem;\r\n            color: #999999;\r\n            line-height: .2rem;\r\n            text-decoration: line-through;\r\n        }\r\n    }\r\n    &__number{\r\n        position: absolute;\r\n        bottom: .12rem;\r\n        right: 0;\r\n        font-size: 14px;\r\n        color: #333333;\r\n        line-height: 16px;\r\n        &__add,&__sub{\r\n            display: inline-block;\r\n            width: .2rem;\r\n            height: .2rem;\r\n            border-radius: 50%;\r\n            text-align: center;\r\n            line-height: .18rem;\r\n            font-size: .2rem;\r\n        }\r\n        &__add{\r\n            margin-left: .1rem;\r\n            border: 1px solid #666666 ;\r\n            color: #666666\r\n        }\r\n        &__sub{\r\n            margin-right: .1rem;\r\n            border: 1px solid #0091FF ;\r\n            background: #0091FF;\r\n            color: #FFFFFF;\r\n        }\r\n    }\r\n}\r\n}\r\n.check{\r\n    display: flex;\r\n    &__icon{\r\n        position: relative;\r\n        width: .76rem;\r\n        height: .49rem;\r\n        &__img{\r\n            display: block;\r\n            width: .28rem;\r\n            height: .26rem;\r\n            margin: .11rem auto .12rem auto\r\n        }\r\n        &__tag{\r\n            position: absolute;\r\n            top: .04rem;\r\n            right: .16rem;\r\n            min-width: .2rem;\r\n            height: .2rem;\r\n            border-radius: .1rem;\r\n            line-height: .2rem;\r\n            text-align: center;\r\n            transform: scale(.5,.5);\r\n            background-color: #E93B3B;\r\n            color: #FFFFFF;\r\n            transform-origin: left center;\r\n        }\r\n    }\r\n    &__info{\r\n        flex: 1;\r\n        padding-left: .08rem;\r\n        font-size: .12rem;\r\n        color: #333333;\r\n        &__price{\r\n            line-height: .49rem;\r\n            font-size: .18rem;\r\n            color: #E93B3B;\r\n        }\r\n    }\r\n    &__btn{\r\n        width: .98rem;\r\n        line-height: .49rem;\r\n        text-align: center;\r\n        background: #4FB0F9;\r\n        font-size: .14rem;\r\n        color: #FFFFFF;\r\n    }\r\n}\r\n\r\n</style>"],"sourceRoot":""}]}