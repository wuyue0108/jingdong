ter();\r\n    const handleBackClick = () => {\r\n        router.back();\r\n    }\r\n    return {handleBackClick}\r\n}\r\n\r\nexport default {\r\n    name:\"Shop\",\r\n    components :{ ShopInfo,Content,Cart},\r\n    setup () {\r\n        const hiddenBottom = false;\r\n        const {item,getItemData} = useShopInfoEffect();\r\n        const {handleBackClick} = usehandleBackEffect();\r\n        getItemData();\r\n       return {hiddenBottom,item,getItemData,handleBackClick}\r\n\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/variable.scss';\r\n.wrapper{\r\n    margin:0 .18rem \r\n}\r\n.search{\r\n    display: flex;\r\n    width: 100%;\r\n    height: .32rem;\r\n    margin: .14rem 0 .04rem 0;\r\n    &__back{\r\n        width: .38rem;\r\n        line-height: .32rem;\r\n        color: #B6B6B6;\r\n        font-size: .24rem;\r\n    }\r\n    &__content{\r\n        display: flex;\r\n        flex: 1;\r\n        height: .32rem;\r\n        &__icon{\r\n            width: .44rem;\r\n            line-height: .32rem;\r\n            font-size: .18rem;\r\n            text-align: center;\r\n            color: $input-bgColor;\r\n        }\r\n        &__input{\r\n            flex: 1;\r\n            display: block;\r\n            background: none;\r\n            outline: none;\r\n            border: none;\r\n            font-size: 14px;\r\n            color: #333333;\r\n            line-height: 16px;\r\n        }\r\n    }\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}