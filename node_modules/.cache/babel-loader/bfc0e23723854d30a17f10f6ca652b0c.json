AAA,Q,GAEP,CANN,CAkBE,EAXF,YAAA,CAOM,KAPN,EAAA,UAAA,EAOM,C,gBANF,YAAA,CAKc,OALd,EAKc;AAJd,IAAA,KAAK,EAAC,yBAIQ;AAJkB,IAAA,IAAI,EAAC,UAIvB;AAHd,IAAA,WAAW,EAAC,OAGE;;aAFL,MAAA,CAAA,QAAA,GAAQ,M;MAEH;AADd,IAAA,YAAY,EAAC,cACC;AAAd,IAAA,IAAI,EAAC,EAAS;AAAN,IAAA,EAAE,EAAC;AAAG,GALd,E,IAAA,E,GAAA,C,iBAGS,MAAA,CAAA,Q,GAGP,CAPN,CAWE,EAHF,YAAA,CAAgE,KAAhE,EAAgE;AAA1D,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,MAAA,CAAA,WAAA,IAAA,MAAA,CAAA,WAAA,OAAA,MAAA,YAAF;AAAA,KAAA,CAAqD;AAAtC,IAAA,KAAK,EAAC;AAAgC,GAAhE,EAAwD,IAAxD,CAGE,EAFF,YAAA,CAAmE,KAAnE,EAAmE;AAA7D,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,MAAA,CAAA,cAAA,IAAA,MAAA,CAAA,cAAA,OAAA,MAAA,YAAF;AAAA,KAAA,CAAwD;AAAtC,IAAA,KAAK,EAAC;AAAgC,GAAnE,EAAyD,MAAzD,CAEE,EADW,MAAA,CAAA,S,kBAAb,YAAA,CAAiD,gBAAjD,EAAiD;UAAA;AAAxB,IAAA,OAAO,EAAI,MAAA,CAAA;AAAa,GAAjD,E,IAAA,E,CAAA,E,WAAA,C,iCACE,CApBN,C","sourcesContent":["<template>\r\n    <div class=\"wrapper\">\r\n        <img class=\"wrapper__img\" src=\"http://www.dell-lee.com/imgs/vue3/user.png\">\r\n        <div class=\"wrapper__input\">\r\n            <input \r\n            class=\"wrapper__input__content\" type=\"text\" \r\n            placeholder=\"请输入用户名\" \r\n            v-model=\"username\"\r\n            name=\"\" id=\"\">\r\n        </div>\r\n        <div class=\"wrapper__input\">\r\n            <input \r\n            class=\"wrapper__input__content\" type=\"password\" \r\n            placeholder=\"请输入密码\" \r\n            v-model=\"password\"\r\n            autocomplete=\"new-password\"\r\n            name=\"\" id=\"\">\r\n        </div>\r\n        <div @click=\"handleLogin\" class=\"wrapper__login-button\">登录</div>\r\n        <div @click=\"handleRegister\" class=\"wrapper__login-link\">立即注册</div>\r\n        <Toast v-if=\"toastShow\" :message = 'toastText' />\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { reactive,toRefs} from 'vue'\r\nimport {useRouter} from 'vue-router'\r\nimport {post} from '../../utils/request.js'\r\nimport Toast,{useToastEffect}  from '../../components/Toast.vue'\r\n\r\nconst useLoginEffect = (changeToast) => {\r\n    const data = reactive({\r\n        username: '',\r\n        password: '',\r\n    });\r\n    const router = useRouter();\r\n    const handleLogin = async () => {\r\n        const {username,password} = data;\r\n        const flag = (username !='')&& (password != '')\r\n        if(flag){\r\n            try {\r\n            const result = await post('/api/user/login', {\r\n                username:data.username,\r\n                password:data.password\r\n            })\r\n                // console.log(result)\r\n            if(result.errno === 0){\r\n                localStorage.isLogin = true;\r\n                router.push({name:'Home'})      \r\n            }else{\r\n                changeToast('登录失败')\r\n            }\r\n            } \r\n            catch (error) {\r\n            changeToast('请求失败')\r\n            }\r\n        }else{\r\n            changeToast('请输入信息')\r\n        }\r\n        \r\n    };\r\n    const {username,password} = toRefs(data);\r\n    return {username,password,handleLogin }\r\n}\r\n\r\nconst useRegisterEffect = ()=>{\r\n    const router = useRouter();\r\n    const handleRegister =()=>{\r\n        router.push({name:'Register'})\r\n    }\r\n    return {handleRegister}\r\n}\r\n\r\n\r\nexport default {\r\n    name:\"Login\",\r\n    components :{\r\n        Toast\r\n    },\r\n    setup(){\r\n        const {toastShow ,toastText, changeToast} = useToastEffect();\r\n        const {username,password,handleLogin } = useLoginEffect(changeToast);\r\n        const {handleRegister} = useRegisterEffect();\r\n        return {toastShow ,toastText, changeToast,\r\n                username,password,handleLogin,\r\n                handleRegister\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n\r\n<style lang=\"scss\" scoped>\r\n.wrapper{\r\n    position: absolute;\r\n    top: 50%;\r\n    left: 0;\r\n    right: 0;\r\n    transform: translateY(-50%);\r\n    &__img{\r\n    display: block;\r\n    width: .66rem;\r\n    height: .66rem;\r\n    margin: 0 auto .4rem auto;\r\n    }\r\n    &__input{\r\n        box-sizing: border-box;\r\n        width: 2.98rem;\r\n        height: .48rem;\r\n        background: #F9F9F9;\r\n        border: 1px solid rgba(0,0,0,0.10);\r\n        border-radius: 6px;\r\n        border-radius: 6px;\r\n        margin: 0 auto .16rem auto;\r\n        padding: 0 0 0 .12rem;\r\n        &__content{\r\n            font-size: .16rem;\r\n            line-height: .43rem;\r\n            width: 100%;\r\n            border: none;\r\n            background: none;\r\n            outline: none;\r\n            ::placeholder{\r\n                font-size: 16px;\r\n                color: rgba(0,0,0,0.50);\r\n                line-height: 24px;\r\n            }\r\n        }\r\n    }\r\n    &__login-button{\r\n        line-height: .48rem;\r\n        margin: .32rem .4rem .16rem .4rem;\r\n        background: #0091FF;\r\n        box-shadow: 0 .04rem .08rem 0 rgba(0,145,255,0.32);\r\n        border-radius: .04rem;\r\n        border-radius: .04rem;\r\n        font-size: .16rem;\r\n        color: #FFFFFF;\r\n        text-align: center;\r\n    }\r\n    &__login-link{\r\n        text-align: center;\r\n        font-size: 14px;\r\n        color: rgba(0,0,0,0.50);\r\n    }\r\n}\r\n</style>\r\n\r\n\r\n\r\n"],"sourceRoot":""}]}